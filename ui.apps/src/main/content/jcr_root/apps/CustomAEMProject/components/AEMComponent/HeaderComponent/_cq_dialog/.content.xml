<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Component Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <logoImage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                fieldLabel="Logo Image"
                name="./logoImage"/>
            <homePageTitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Home Page Title"
                name="./homePageTitle"/>
            <homePageUrl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                fieldLabel="Home Page Url"
                name="./homePageUrl"/>
            <aboutUsTitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="About Us Title"
                name="./aboutUsTitle"/>
            <aboutUsUrl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                fieldLabel="About Us Page Url"
                name="./aboutUsPageUrl"/>
            <aboutUsSubNavigation
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                composite="{Boolean}true">
                <field
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container"
                    name="./aboutUsSubNavigation">
                    <items jcr:primaryType="nt:unstructured">
                        <subNavTitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="About Us Sub Nav Title"
                            name="./aboutUsSubNavTitle"/>
                        <subNavUrl
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldLabel="About Us Sub Nav Url"
                            name="./aboutUsSubNavUrl"/>
                        <subNavChild
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                            composite="{Boolean}true">
                            <field
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                name="./subNavChild">
                                <items jcr:primaryType="nt:unstructured">
                                    <subNavChildTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="About Us Sub Nav Child Title"
                                        name="./subNavChildTitle"/>
                                    <subNavChildUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="About Us Sub Nav Child Url"
                                        name="./subNavChildUrl"/>
                                </items>
                            </field>
                        </subNavChild>
                    </items>
                </field>
            </aboutUsSubNavigation>
            <signupTitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Signup Page Title"
                name="./signupPageTitle"/>
            <signupUrl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                fieldLabel="Signup Page Url"
                name="./signupPageUrl"/>
            <loginTitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Login Page Title"
                name="./loginPageTitle"/>
            <loginUrl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                fieldLabel="login Page Url"
                name="./loginPageUrl"/>
        </items>
    </content>
</jcr:root>
